<table>
  <thead>
  <tr>
    <th>Date</th>
    <th>Description</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @pet_incidents.each do |pet_incident| %>
      <tr>
        <td><%= pet_incident.incident.incident_date.strftime("%m/%d/%Y") rescue ""%></td>
        <!--<td><%= pet_incident.pet.pet_name rescue "" %></td>-->
        <td><%= pet_incident.incident.incident_description %></td>
        <td><%= link_to 'Show', incidents_show_path(id: pet_incident.incident.id), :class => "btn btn-info btn-xs" %></td>
        <td><%= link_to 'Edit', edit_incident_path(id: pet_incident.incident.id), :class => "btn btn-warning btn-xs"  %></td>
        <!--<td><%= link_to 'Destroy', pet_incident, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
      </tr>
  <% end %>
  </tbody>
</table>