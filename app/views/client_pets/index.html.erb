<p id="notice"><%= notice %></p>

<h1>Listing Client Pets</h1>

<table>
  <thead>
    <tr>
      <th>Client</th>
      <th>Pet</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @client_pets.each do |client_pet| %>
      <tr>
        <td><%= client_pet.client %></td>
        <td><%= client_pet.pet %></td>
        <td><%= link_to 'Show', client_pet %></td>
        <td><%= link_to 'Edit', edit_client_pet_path(client_pet) %></td>
        <td><%= link_to 'Destroy', client_pet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
