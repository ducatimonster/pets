      <thead>
      <tr>
        <th>Note type</th>
        <th colspan="3"></th>
      </tr>
      </thead>

      <tbody>
      <% @note_types.each do |note_type| %>
          <tr>
            <td><%= note_type.note_type %></td>
            <!--<td><%= link_to 'Show', note_type, :class=>"btn btn-info sm" %></td>-->
