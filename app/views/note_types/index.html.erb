            <!--<td><%= link_to 'Destroy', note_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
          </tr>
      <% end %>
      </tbody>
    </table>
    <%= will_paginate @pets, renderer: BootstrapPagination::Rails %>

