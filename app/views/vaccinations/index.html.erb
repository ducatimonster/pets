<p id="notice"><%= notice %></p>

<h1>Vaccination List</h1>

<div class="panel panel-default">
  <div class="panel-body">
    <table class="table table-striped">
      <thead>
      <tr>
        <th>Vaccination name</th>
        <th colspan="3"></th>
      </tr>
      </thead>

      <tbody>
      <% @vaccinations.each do |vaccination| %>
          <tr>
            <td><%= vaccination.vaccination_name %></td>
            <!--<td><%= link_to 'Show', vaccination %></td>-->
            <td><%= link_to 'Edit', edit_vaccination_path(vaccination), :class=>'btn btn-warning sm' %></td>
            <!--<td><%= link_to 'Destroy', vaccination, method: :delete, data: { confirm: 'Are you sure?' } %></td>-->
          </tr>
      <% end %>
      </tbody>
    </table>
    <%= link_to 'New Vaccination', new_vaccination_path, :class=> "btn btn-default" %>
  </div>
</div>



