<p id="notice"><%= notice %></p>

<h1>Listing Employee Services</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Service</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_services.each do |employee_service| %>
      <tr>
        <td><%= employee_service.employee %></td>
        <td><%= employee_service.service %></td>
        <td><%= link_to 'Show', employee_service %></td>
        <td><%= link_to 'Edit', edit_employee_service_path(employee_service) %></td>
        <td><%= link_to 'Destroy', employee_service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

